[ Config ]

name=SmartME
version=1.0
description=OpenTTY Toolkit

include=forge

config=execute set SDK=1.0; about sdkme;
command=sdk,mp

sdk=execute x11 list sdkme; 
mp=execute open http://mp.nnchan.ru;

file=sdkme-index.html

sdkme-index.html=HTTP/1.1 200 OK\nContent-Type: text/html\nConnection: close\n\n\n<!DOCTYPE html>\n<html lang="en">\n<head>\n    \n<title>SmartME HTML</title>\n</head>\n<body>\n	Put HTML Content here\n</body>\n</html>


[ DISPLAY ]

list.title=SmartME SDK
list.content=Forge,Create HTML Page,About SDK
list.back=

Forge=execute forge; 
Create HTML Page=execute nano sdkme-index.html;
About SDK=execute set OLD_TITLE=$TITLE; title About SmartME; warn SmartME $SDK\nOpenTTY Toolkit; title $OLD_TITLE; unset OLD_TITLE;
