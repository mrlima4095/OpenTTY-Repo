[ Config ]

name=Armitage
version=1.0
description=OpenTTY Exploits 

include=/java/lib/netkit

config=execute about armitage
commmand=armitage

armitage=execute x11 make armitage;


[ DISPLAY ]

screen.title=Armitage
screen.content=
screen.button=Tools
screen.button.cmd=execute x11 list armitage 
screen.back=

quest.title=Armitage
quest.label=IP Adress
quest.key=INPUT
quest.cmd=execute $TOOL $INPUT; unset TOOL; unset INPUT;

list.title=Armitage Tools
list.content=Connect (Remote),Connect - Server,Get Local Adress,Search IP Info,IP Generator,HTTP Requester,Charge Server IP
list.back=execute x11 make armitage

Connect (Remote)=execute set TOOL=nc; x11 quest armitage;
Connect - Server=execute nc $REPO;
Get Local Adress=execute ifconfig; set OLD_TITLE=$TITLE; title IP Adress; if ($OUTPUT == null) warn Netork Error; if ($OUTPUT != null) warn $OUTPUT; title $OLD_TITLE; unset OLD_TITLE;
Search IP Info=execute set TOOL=fw; x11 quest armitage;
IP Generator=execute query $REPO genip; set OLD_TITLE=$TITLE; title IP Generator; warn $OUTPUT; set GENIP=$OUTPUT; title $OLD_TITLE; unset OLD_TITLE;
HTTP Requester=execute buff query $REPO http;
Charge Server IP=execute x11 quest /java/lib/yang;
